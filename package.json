{
  "name": "nbase",
  "version": "1.0.0",
  "description": "practice node.js, express, sequelize ... and so more.",
  "main": "index.js",
  "scripts": {
    "serve": "node -r ts-node/register -r tsconfig-paths/register dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migration": "ts-node -T -r tsconfig-paths/register src/migration/index.ts",
    "practice": "ts-node -T -r tsconfig-paths/register src/practice.ts",
    "tsc": "tsc -p ./tsconfig.json",
    "tsc:watch": "tsc -p ./tsconfig.json --watch",
    "nodemon": "nodemon",
    "ts-node": "ts-node -T -r tsconfig-paths/register src/index.ts",
    "local:build": "docker-compose -f docker-compose.local.yml build --no-cache && docker-compose -f docker-compose.local.yml up",
    "docker:build": "docker build -t kimjbstar/nbase -f Dockerfile-web .",
    "docker:push": "docker push kimjbstar/nbase",
    "aws:deploy": "aws ecs update-service --cluster=ecs-cluster-01 --service=ecs-service-web --force-new-deployment"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kimjbstar/nbase.git"
  },
  "author": "kimjbstar@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@types/jsonwebtoken": "^8.3.8",
    "@types/morgan": "^1.9.0",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "inflection": "^1.12.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.5",
    "sequelize-typescript": "^1.1.0",
    "ts-node": "^8.8.1",
    "typedi": "^0.8.0",
    "typescript": "^3.8.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.29",
    "@types/express": "^4.17.2",
    "@types/inflection": "^1.5.28",
    "@types/node": "^13.7.6",
    "@types/validator": "^12.0.1",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^5.5.1",
    "tsconfig-paths": "^3.9.0"
  }
}
