{
  "name": "just1s-backend",
  "version": "1.0.0",
  "description": "just1s API Server",
  "main": "index.js",
  "scripts": {
    "start:local": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/src/main.js",
    "build": "tsc -p ./tsconfig.json",
    "webpack": "webpack --display=verbose",
    "build:compose": "docker-compose build --no-cache && docker-compose up",
    "build:docker": "docker build -t kimjbstar/just1s-backend .",
    "deploy:docker": "docker push kimjbstar/just1s-backend",
    "deploy:aws": "aws ecs update-service --cluster=ecs-cluster-01 --service=service-back-1 --force-new-deployment",
    "practice": "ts-node -r tsconfig-paths/register src/practice.ts",
    "test": "mocha --debug -r ts-node/register -r tsconfig-paths/register ./**/*.test.ts --timeout 20000 --exit",
    "dumpdata": "ts-node -T -r tsconfig-paths/register src/dumpdata.ts",
    "scaffold": "ts-node -T -r tsconfig-paths/register src/scaffold/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "sequelize:makemigration": "ts-node -T -r tsconfig-paths/register src/scripts/makemigration.ts"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kimjbstar/just1s-backend.git"
  },
  "author": "kimjbstar@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^1.0.0-alpha.1",
    "@nestjs/common": "^7.0.5",
    "@nestjs/core": "^7.0.5",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.5",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/typeorm": "^7.0.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "aws-serverless-express": "^3.3.8",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "inflection": "^1.12.0",
    "mocha": "^7.1.1",
    "moment": "^2.26.0",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.24",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.9",
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.2",
    "@types/inflection": "^1.5.28",
    "@types/mocha": "^7.0.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.13.4",
    "@types/qs": "^6.9.3",
    "@types/validator": "^12.0.1",
    "aws-lambda": "^1.0.6",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "glob": "^7.1.6",
    "handlebars": "^4.7.6",
    "prettier": "^2.0.4",
    "serverless-layers": "^2.2.4",
    "serverless-offline": "^6.4.0",
    "serverless-plugin-optimize": "^4.1.4-rc.1",
    "serverless-webpack": "^5.3.2",
    "sqlite3": "^4.1.1",
    "ts-loader": "^7.0.5",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2"
  }
}
