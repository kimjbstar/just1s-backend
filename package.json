{
  "name": "just1s-backend",
  "version": "1.0.0",
  "description": "just1s API Server",
  "main": "index.js",
  "scripts": {
    "start:local": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "start:prod": "node -r ./tsconfig-paths-bootstrap.js dist/src/main.js",
    "build": "tsc -p ./tsconfig.json",
    "practice": "ts-node -r tsconfig-paths/register src/practice.ts",
    "local:build": "docker-compose -f docker-compose.local.yml build --no-cache && docker-compose -f docker-compose.local.yml up",
    "docker:build": "docker build -t kimjbstar/just1s-backend .",
    "docker:push": "docker push kimjbstar/just1s-backend",
    "aws:deploy": "aws ecs update-service --cluster=ecs-cluster-01 --service=service-back-1 --force-new-deployment",
    "old:serve": "node -r ts-node/register -r tsconfig-paths/register dist/src/index.js",
    "test": "mocha --debug -r ts-node/register -r tsconfig-paths/register ./**/*.test.ts --timeout 20000 --exit",
    "dumpdata": "ts-node -T -r tsconfig-paths/register src/dumpdata.ts",
    "scaffold": "ts-node -T -r tsconfig-paths/register src/scaffold/index.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "sequelize:makemigration": "ts-node -T -r tsconfig-paths/register src/scripts/makemigration.ts",
    "start:dev:nodemon": "nodemon",
    "start:debug:nodemon": "nodemon --config nodemon-debug.json"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kimjbstar/just1s-backend.git"
  },
  "author": "kimjbstar@gmail.com",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^7.0.5",
    "@nestjs/core": "^7.0.5",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.5",
    "@nestjs/serve-static": "^2.1.0",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/typeorm": "^7.0.0",
    "@types/morgan": "^1.9.0",
    "@types/passport": "^1.0.2",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "aws-sdk": "^2.658.0",
    "change-case": "^4.1.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^3.22.0",
    "inflection": "^1.12.0",
    "mocha": "^7.1.1",
    "moment": "^2.26.0",
    "morgan": "^1.9.1",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pluralize": "^8.0.0",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typeorm": "^0.2.24",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.0.9",
    "@types/bluebird": "^3.5.30",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.2",
    "@types/inflection": "^1.5.28",
    "@types/mocha": "^7.0.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.13.4",
    "@types/qs": "^6.9.3",
    "@types/validator": "^12.0.1",
    "chai": "^4.2.0",
    "handlebars": "^4.7.6",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4",
    "sqlite3": "^4.1.1",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5"
  }
}
